	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#590"
			}
		},
		{
			"scope": "comment.block.documentation",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"constant",
				"constant.numeric",
				"constant.language",
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#48d"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"entity",
				"variable.other.enummember"
			],
			"settings": {
				"foreground": "#0cc"
			}
		},
		{
			"scope": [
				"entity.name.section"
			],
			"settings": {
				"foreground": "#48d"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#48d"
			}
		},
		{
			"scope": "entity.name.function.operator",
			"settings": {
				"foreground": "#e5d"
			}
		},
		{
			"scope": "entity.name.namespace",
			"settings": {
				"foreground": "#099"
			}
		},
